{
  "target": "bmv2",
  "p4info": "build/routing.p4.p4info.txt",
  "bmv2_json": "build/routing.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_ack_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": {}
    },
    {
      "table": "MyIngress.ipv4_ack_lpm",
      "match": {
      	"hdr.ip_hdr.dstAddr": ["10.0.1.0", 24]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
				"dstAddr": "08:00:00:00:01:11",
				"port": 1
    	}
    },
    {
      "table": "MyIngress.ipv4_ack_lpm",
      "match": {
      	"hdr.ip_hdr.dstAddr": ["10.0.2.0", 24]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
				"dstAddr": "08:00:00:00:02:22",
				"port": 2
    	}
    }
  ]
}
